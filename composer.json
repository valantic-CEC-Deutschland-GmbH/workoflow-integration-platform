{
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "runtime/frankenphp-symfony",
                "version": "1.1.0",
                "source": {
                    "url": "https://github.com/php-runtime/frankenphp-symfony.git",
                    "type": "git",
                    "reference": "main"
                },
                "autoload": {
                    "psr-4": {
                        "Runtime\\FrankenPhpSymfony\\": "src/"
                    }
                },
                "require": {
                    "php": ">=8.1",
                    "symfony/dependency-injection": "^5.4 || ^6.0 || ^7.0 || ^8.0",
                    "symfony/http-kernel": "^5.4 || ^6.0 || ^7.0 || ^8.0",
                    "symfony/runtime": "^5.4 || ^6.0 || ^7.0 || ^8.0"
                }
            }
        }
    ],
    "require": {
        "php": ">=8.4",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-sodium": "*",
        "aws/aws-sdk-php": "^3.0",
        "doctrine/dbal": "^4",
        "doctrine/doctrine-bundle": "^3",
        "doctrine/doctrine-migrations-bundle": "^4",
        "doctrine/orm": "^3.0",
        "firebase/php-jwt": "^7.0",
        "guzzlehttp/guzzle": "^7.8",
        "helpscout/oauth2-hubspot": "^0.2.2",
        "knpuniversity/oauth2-client-bundle": "^2.18",
        "league/commonmark": "^2.8",
        "league/oauth2-google": "^4.0",
        "lexik/jwt-authentication-bundle": "^3.1",
        "monolog/monolog": "^3.0",
        "nelmio/cors-bundle": "^2.5",
        "phpdocumentor/reflection-docblock": "^5.4",
        "phpoffice/phpspreadsheet": "^5.0",
        "phpoffice/phpword": "^1.0",
        "phpstan/phpdoc-parser": "^2.0",
        "predis/predis": "^3.0",
        "ramsey/uuid": "^4.7",
        "runtime/frankenphp-symfony": "*",
        "smalot/pdfparser": "^2.0",
        "symfony/asset": "8.0.*",
        "symfony/console": "8.0.*",
        "symfony/doctrine-messenger": "8.0.*",
        "symfony/dotenv": "8.0.*",
        "symfony/expression-language": "8.0.*",
        "symfony/flex": "^2",
        "symfony/form": "8.0.*",
        "symfony/framework-bundle": "8.0.*",
        "symfony/http-client": "8.0.*",
        "symfony/intl": "8.0.*",
        "symfony/mime": "8.0.*",
        "symfony/monolog-bundle": "^4.0",
        "symfony/process": "8.0.*",
        "symfony/property-access": "8.0.*",
        "symfony/property-info": "8.0.*",
        "symfony/runtime": "8.0.*",
        "symfony/security-bundle": "8.0.*",
        "symfony/serializer": "8.0.*",
        "symfony/string": "8.0.*",
        "symfony/translation": "8.0.*",
        "symfony/twig-bundle": "8.0.*",
        "symfony/uid": "8.0.*",
        "symfony/ux-twig-component": "^2.31",
        "symfony/validator": "8.0.*",
        "symfony/web-link": "8.0.*",
        "symfony/webpack-encore-bundle": "^2.2",
        "symfony/yaml": "8.0.*",
        "thenetworg/oauth2-azure": "^2.2",
        "twig/extra-bundle": "^2.12|^3.0",
        "twig/string-extra": "^3.23",
        "twig/twig": "^2.12|^3.0"
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^4.2",
        "phpstan/extension-installer": "*",
        "phpstan/phpstan": "*",
        "phpstan/phpstan-symfony": "*",
        "phpunit/phpunit": "^13.0",
        "squizlabs/php_codesniffer": "*",
        "symfony/browser-kit": "8.0.*",
        "symfony/css-selector": "8.0.*",
        "symfony/debug-bundle": "8.0.*",
        "symfony/maker-bundle": "^1.0",
        "symfony/phpunit-bridge": "^8.0",
        "symfony/stopwatch": "8.0.*",
        "symfony/web-profiler-bundle": "8.0.*"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true,
            "symfony/flex": true,
            "symfony/runtime": true,
            "phpstan/extension-installer": true
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*",
        "symfony/polyfill-php82": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "phpstan": "vendor/bin/phpstan analyse --memory-limit=1G",
        "phpcs": "vendor/bin/phpcs",
        "phpcbf": "vendor/bin/phpcbf",
        "code-check": [
            "@phpstan",
            "@phpcs"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "8.0.*",
            "docker": true
        }
    }
}
